{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "106",
          "firefox": "106",
          "chrome": "106"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-import-assertions",
    "babel-plugin-inline-json-import",
    "@babel/plugin-proposal-private-methods"
  ]
}
